<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "..\..\..\Fabulae\core\dtd\location.dtd">
<location>
	<triggers>
		<onEntry>
			<action>
				<logMessage message="wmMessages.json#enteringFreshWater" logType="survival" />
			</action>
		</onEntry>
		<onExit>
			<action>
				<logMessage message="wmMessages.json#leavingFreshWater" logType="survival" />
			</action>
		</onExit>
	</triggers>
	<activityModifiers>
		<waterSearching>
			100
		</waterSearching>
	</activityModifiers>
</location>